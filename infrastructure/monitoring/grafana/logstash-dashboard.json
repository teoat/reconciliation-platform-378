{
  "dashboard": {
    "title": "Logstash Monitoring",
    "tags": ["logstash", "monitoring", "elastic"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Events Rate (per second)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(logstash_pipeline_events_in_total[5m])",
            "legendFormat": "Events In",
            "refId": "A"
          },
          {
            "expr": "rate(logstash_pipeline_events_out_total[5m])",
            "legendFormat": "Events Out",
            "refId": "B"
          },
          {
            "expr": "rate(logstash_pipeline_events_filtered_total[5m])",
            "legendFormat": "Events Filtered",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Events/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "JVM Heap Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "(logstash_jvm_memory_used_bytes{area=\"heap\"} / logstash_jvm_memory_max_bytes{area=\"heap\"}) * 100",
            "legendFormat": "Heap Used %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "max": 100,
            "min": 0,
            "label": "Percentage"
          }
        ],
        "thresholds": [
          {
            "value": 80,
            "colorMode": "warning",
            "op": "gt"
          },
          {
            "value": 90,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 3,
        "title": "Queue Size",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "logstash_pipeline_queue_events_count",
            "legendFormat": "Queue Events",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Events"
          }
        ]
      },
      {
        "id": 4,
        "title": "Total Events",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "logstash_pipeline_events_in_total",
            "legendFormat": "Total In",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Total Events Out",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
        "targets": [
          {
            "expr": "logstash_pipeline_events_out_total",
            "legendFormat": "Total Out",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "title": "JVM Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "logstash_jvm_memory_used_bytes{area=\"heap\"}",
            "legendFormat": "Heap Used",
            "refId": "A"
          },
          {
            "expr": "logstash_jvm_memory_max_bytes{area=\"heap\"}",
            "legendFormat": "Heap Max",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory"
          }
        ]
      },
      {
        "id": 7,
        "title": "Pipeline Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "logstash_pipeline_queue_max_events_count - logstash_pipeline_queue_events_count",
            "legendFormat": "Queue Capacity Remaining",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Events"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}

