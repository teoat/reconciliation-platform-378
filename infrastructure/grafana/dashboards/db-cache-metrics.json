{
  "dashboard": {
    "title": "Database & Cache Metrics",
    "tags": ["reconciliation", "database", "cache", "performance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "DB Query Duration (p95)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, reconciliation_db_query_duration_seconds_bucket)",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, reconciliation_db_query_duration_seconds_bucket)",
            "legendFormat": "p50",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, reconciliation_db_query_duration_seconds_bucket)",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.15, "color": "yellow"},
                {"value": 0.25, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 2,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"x": 8, "y": 0, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "rate(reconciliation_cache_hits_total[5m]) / (rate(reconciliation_cache_hits_total[5m]) + rate(reconciliation_cache_misses_total[5m]))",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.6, "color": "yellow"},
                {"value": 0.8, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "graphMode": "area"
        }
      },
      {
        "id": 3,
        "title": "Connection Pool Utilization",
        "type": "stat",
        "gridPos": {"x": 16, "y": 0, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "reconciliation_db_pool_connections_active / reconciliation_db_pool_connections_total",
            "legendFormat": "Utilization",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.85, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Cache Hits by Type",
        "type": "graph",
        "gridPos": {"x": 0, "y": 6, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(reconciliation_cache_hits_total[5m])",
            "legendFormat": "{{key_type}} ({{cache_level}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Hits/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Cache Misses by Type",
        "type": "graph",
        "gridPos": {"x": 12, "y": 6, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(reconciliation_cache_misses_total[5m])",
            "legendFormat": "{{key_type}} ({{cache_level}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Misses/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Connection Pool Stats",
        "type": "graph",
        "gridPos": {"x": 0, "y": 14, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "reconciliation_db_pool_connections_active",
            "legendFormat": "Active",
            "refId": "A"
          },
          {
            "expr": "reconciliation_db_pool_connections_idle",
            "legendFormat": "Idle",
            "refId": "B"
          },
          {
            "expr": "reconciliation_db_pool_connections_total",
            "legendFormat": "Total",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "DB Query Duration by Route",
        "type": "heatmap",
        "gridPos": {"x": 12, "y": 14, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(reconciliation_db_query_duration_seconds_bucket[5m])) by (le, route)",
            "format": "heatmap",
            "refId": "A"
          }
        ],
        "heatmap": {
          "yAxis": {"unit": "s"}
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "iconColor": "blue",
          "type": "dashboard"
        }
      ]
    }
  }
}


