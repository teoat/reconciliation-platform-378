{
  "name": "reconciliation-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "build:cache": "vite build --mode production --cache-dir .vite-cache",
    "build:clean": "rm -rf dist .vite-cache node_modules/.tmp && npm run build:cache",
    "build:time": "time npm run build",
    "build:analyze": "tsc && vite build --mode production && npx vite-bundle-visualizer",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run src/__tests__",
    "test:integration": "vitest --run src/__tests__/integration",
    "test:e2e": "playwright test",
    "test:security": "vitest --run src/__tests__/security",
    "test:performance": "vitest --run src/__tests__/performance",
    "test:accessibility": "vitest --run src/__tests__/accessibility",
    "cache:clean": "rm -rf .vite-cache node_modules/.tmp",
    "devtools:diag": "cd tools/cdp-runner && npm run diag",
    "devtools:diag:full": "cd tools/cdp-runner && npm run diag:full",
    "devtools:diag:quick": "cd tools/cdp-runner && npm run diag:quick",
    "cdp:diagnostics": "npm run devtools:diag:full || true",
    "openapi:gen": "openapi-typescript ../../backend/openapi.yaml -o src/types/generated/api.ts",
    "openapi:check": "npm run openapi:gen && git diff --exit-code -- src/types/generated/api.ts"
  },
  "dependencies": {
    "@elastic/apm-rum": "^5.17.0",
    "@hookform/resolvers": "^3.3.2",
    "@reduxjs/toolkit": "^2.9.1",
    "@tanstack/react-virtual": "^3.13.12",
    "axios": "^1.6.0",
    "better-auth": "^1.0.0",
    "dompurify": "^3.0.6",
    "lucide-react": "^0.294.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.47.0",
    "react-redux": "^9.0.0",
    "react-router-dom": "^6.8.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "openapi-typescript": "^6.7.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.0",
    "terser": "^5.36.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-compression2": "^2.3.1",
    "vitest": "^1.0.4"
  }
}
