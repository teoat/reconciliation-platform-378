{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/Users/Arief/Documents/GitHub/reconciliation-platform-378"],
      "env": {}
    },
    "postgres": {
      "command": "npx",
      "args": ["-y", "@ahmetkca/mcp-server-postgres", "postgresql://postgres:postgres_pass@localhost:5432/reconciliation_app"],
      "env": {}
    },
    "prometheus": {
      "command": "npx",
      "args": ["-y", "@wkronmiller/prometheus-mcp-server"],
