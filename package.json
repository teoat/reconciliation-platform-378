{
  "name": "378-data-evidence-reconciliation-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 1000",
    "build": "next build",
    "start": "next start -p 1000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "check-bundle-size": "node scripts/check-bundle-size.js",
    "check-bundle-size:strict": "node scripts/check-bundle-size.js --strict",
    "build:verify": "npm run build && npm run check-bundle-size",
    "performance:verify": "bash scripts/verify-performance.sh",
    "db:apply-indexes": "bash scripts/apply-db-indexes.sh",
    "deploy:check": "bash scripts/production-deployment-checklist.sh",
    "remove-console-logs": "bash scripts/remove-console-logs.sh",
    "remove-console-logs:dry-run": "bash scripts/remove-console-logs.sh --dry-run",
    "remove-console-logs:replace": "bash scripts/remove-console-logs.sh --replace-with-logger",
    "audit:debt": "bash scripts/audit-technical-debt.sh",
    "coverage:check": "bash scripts/check-coverage.sh",
    "coverage:report": "npm run test:coverage && bash scripts/check-coverage.sh",
    "files:large": "bash scripts/find-large-files.sh",
    "quality:check": "npm run audit:debt && npm run coverage:check && npm run files:large",
    "performance:test": "node scripts/performance-test.js",
    "performance:baseline": "node scripts/performance-test.js --baseline",
    "performance:ci": "node scripts/performance-test.js --ci",
    "performance:alert": "node scripts/performance-alerts.js",
    "performance:budget:check": "node scripts/performance-budget.js --check",
    "performance:budget:enforce": "node scripts/performance-budget.js --enforce",
    "performance:budget:update": "node scripts/performance-budget.js --update",
    "performance:kpis": "node scripts/performance-kpis.js --check --report",
    "performance:kpis:update": "node scripts/performance-kpis.js --update",
    "performance:monitor": "node scripts/performance-monitor.js",
    "performance:monitor:continuous": "node scripts/performance-monitor.js --continuous 60",
    "performance:ci-check": "node scripts/ci-performance-check.js",
    "code-splitting:analyze": "node scripts/analyze-code-splitting.js --analyze",
    "code-splitting:implement": "node scripts/analyze-code-splitting.js --implement",
    "pwa:validate": "npx pwa-asset-generator public/manifest.json public/icons --background '#F8FAFC' --theme '#3B82F6'",
    "pwa:build": "npm run build && npm run pwa:validate",
    "monitoring:setup": "bash scripts/setup-production-monitoring.sh",
    "bundle:monitor": "bash scripts/bundle-monitor.sh --alert",
    "bundle:monitor:baseline": "bash scripts/bundle-monitor.sh --baseline",
    "baselines:establish": "bash scripts/establish-baselines.sh",
    "reports:review": "bash scripts/review-reports.sh",
    "execution:checklist": "bash scripts/execution-checklist.sh",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@sentry/react": "^10.22.0",
    "lucide-react": "^0.294.0",
    "next": "14.2.33",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-intl": "^6.4.2",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.8.0",
    "recharts": "^3.3.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.2",
    "tailwindcss": "^3.3.0",
    "web-vitals": "^5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/recharts": "^2.0.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.0.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.4.5",
    "typescript": "^5.2.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
